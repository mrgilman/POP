<!DOCTYPE html>
<html>
<head>
  <title>POP</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
  <%= yield :head %>
</head>
<body>
<% if logged_in? %>
<%= render 'layouts/header_in' %>
<% else %>
<%= render 'layouts/header_out' %>
<% end %>
<%= yield %>
<%= render 'layouts/footer' %>
</body>
</html>
